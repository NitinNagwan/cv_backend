              <h1 class="profile-heading">PROFILE</h1>
              <p class="profile-summary">record.cv_details.summary </p>
              <h5 class="quality-heading">KEY QUALITIES</h5>
              <div class="qualities align-checkbox-icon">
                {data?.keyQualities?.map((item, index) => (
                  <div class="align-checkbox-icon" key={index}>
                    
                      <img
                        src="assets/checkcircle.png"
                        width="15px"
                        height="15px"
                        alt="checkCircle"
                      />
                      <p class="quality">{item?.quality?.quality} </p>{" "}
                  </div>
                ))}
              </div>
            </div>
            <div class="expertises">
              <h2 class="experties-heading">EXPERTISE</h2>
              <div class="skills">
                {data?.expertise?.map(({ skill, years }, index) => (
                  <div class="expertise" key={index}>
                    <div class="skill">
                      <img
                        class=" skill-img"
                        src={`assets/${skill}.png`}
                        alt="W"
                        width="25"
                        height="25"
                      />
                      <p style="padding-left: 10px" >{skill}</p>
                    </div>
                    <Rating name="disabled" value={2} disabled />
                    <p class="experience-year">
                      {years}
                      <span class="experience-span">+ years</span>
                    </p>
                    <br />
                  </div>
                ))}
              </div>
            </div>
            {data?.experience[0]?.titles?.length > 0 ? (
              <div class="experiene-section">
                <h3 class="experience-heading">PROFESSIONAL EXPERIENCE</h3>
                <div class="experience-block">
                  {data?.experience?.map(
                    ({ title, startDate, endDate, description }, index) => (
                      <div class="experience-main" key={index}>
                        <p class="experience-date">
                          ({startDate} to {endDate})
                        </p>
                        <p class="experience-tittle">{title}</p>
                        <ul class="experience-ul">
                          {description?.map((desc, index) => (
                            <li class="experience-description" key={index}>
                              {" "}
                              {desc}
                            </li>
                          ))}
                        </ul>
                      </div>
                    )
                  )}
                </div>
              </div>
            ) : null}
            {data?.project[0]?.titles?.length > 0 ? (
              <div class="project-section">
                <h3 class="project-heading">MAJOR PROJECT</h3>
                <div class="project-block">

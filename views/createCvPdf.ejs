        <Watermark text="Valuecoders" textSize={160} rotate={0}>
        <header>
          <div class="main-header">
            <div class="header-name">
              <p class="first-name">
                <%= record.cv_details.header.firstName ? record.cv_details.header.firstName : "" %>
              </p>
              <p class="last-name" style="font-weight: 800;
              font-family: 'Noto Sans';
              font-style: normal;
              font-size: 40px;">
                <%= record.cv_details.header.lastName ?
                record.cv_details.header.lastName : "" %>
              </p>
              <p class="designation">
                <%= record.cv_details.header.designation ?
                record.cv_details.header.designation : "" %>
              </p>
            </div>
            <div class="header-profile">
              <% if (record.cv_details.header.origin)  { %>
                <div class="align-logo">
                  <img
                    src=""
                    width="15px"
                    height="17px"
                    alt=""
                  />
                  <p class="origin">Origin: <%= record.cv_details.header.origin %> </p>
                </div>
             <% } else { %>
                " "
            <%  } %>
              <%  if (record.cv_details.header.experience) { %>
                <div class="align-logo">
                  <img
                    src=""
                    width="20px"
                    height="20px"
                    alt=""
                  />
                  <p class="experience">
                   Experience : <%=  record.cv_details.header.experience %>
                  </p>
                </div>
             <% } else { %>
                 
                <% } %>
              <% if (record.cv_details.header.availability) { %>
                <div class="align-logo">
                  <img
                    src=""
                    width="17px"
                    height="17px"
                    alt=""
                  />
                  <p class="availability">
                    Availability: <%= record.cv_details.header.availability %>
                  </p>
                </div>
            <% } else { %>
               
                
             <% }   %>
            </div>
          </div>
          <% if (record.cv_details.header.image_url){ %>
           <div class="profile-pic">
            <img src = <%= record.cv_details.header.image_url %>  
            width="200px" height="200px" alt="r2" />
         </div>
          <% } else { %>

          <% } %> 
        </header>
